  "source": "pragma solidity ^0.4.24;\n\n\ncontract Ownable {\n    address owner;\n    \n    constructor () public {\n        owner = msg.sender;\n    }\n    \n    modifier onlyOwner () {\n        require (msg.sender == owner);\n        _;\n    }\n}",
